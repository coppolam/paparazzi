<!DOCTYPE module SYSTEM "module.dtd">

<module name="uwb_follower_control" dir="uwb_control">
	<doc>
		<description>
			Control module for follower's flight along a predefined trajectory
		</description>
	</doc>
	<header>
		<file name="uwb_follower_control.h" />
	</header>
	<init fun="uwb_follower_control_init()" />
	<periodic fun="uwb_follower_control_periodic()" freq="20" autorun="TRUE" />
	<makefile>
		<file name="uwb_follower_control.c" />
	</makefile>
</module>
